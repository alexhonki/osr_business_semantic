  <calculationView xsi:type="Calculation:AggregationView" id="AGG_CONTACCTBAL">
    <viewAttributes>
    
      <viewAttribute id="MANDT"/>
      <viewAttribute id="DFKKOP_VKONT"/>
    
      <viewAttribute id="OpenItemBalance" aggregationType="sum"/></viewAttributes>
    <calculatedViewAttributes>
      <calculatedViewAttribute id="AccountBalanceStatus" datatype="NVARCHAR" length="20" expressionLanguage="COLUMN_ENGINE">
        <formula>if(&quot;OpenItemBalance&quot; &gt; 0,'Debit Balance',
if(&quot;OpenItemBalance&quot; = 0,'Nil Balance',
if(&quot;OpenItemBalance&quot; &lt; 0,'Credit Balance',
string(null)
)))</formula>
      </calculatedViewAttribute>
    
      <calculatedViewAttribute id="AccountBalanceRange" datatype="NVARCHAR" length="40" expressionLanguage="COLUMN_ENGINE">
    
        <formula>if(abs(&quot;OpenItemBalance&quot;) &lt; 100.00,'Less than $100.00',
if(abs(&quot;OpenItemBalance&quot;) &gt;= 100.00 AND abs(&quot;OpenItemBalance&quot;) &lt; 1000.01,'$100.00 to $1000.00',
if(abs(&quot;OpenItemBalance&quot;) &gt;= 1000.01 AND abs(&quot;OpenItemBalance&quot;) &lt; 10000.01,'$1,000.01 to $10,000.00',
if(abs(&quot;OpenItemBalance&quot;) &gt;= 10000.01 AND abs(&quot;OpenItemBalance&quot;) &lt; 100000.01,'$10,000.01 to $100,000.00',
if(abs(&quot;OpenItemBalance&quot;) &gt;= 100000.01 AND abs(&quot;OpenItemBalance&quot;) &lt; 500000.01,'$100,000.01 to $500,000.00',
if(abs(&quot;OpenItemBalance&quot;) &gt;= 500000.01 AND abs(&quot;OpenItemBalance&quot;) &lt; 1000000.01,'$500,000.01 to $1,000,000',
if(abs(&quot;OpenItemBalance&quot;) &gt;= 1000000.01,'$1,000,000 and greater',
'Err'
)))))))</formula>
    
      </calculatedViewAttribute>
    </calculatedViewAttributes>
    <restrictedViewAttributes/>
    <input node="P_CONTACCTBAL">
      <mapping xsi:type="Calculation:AttributeMapping" target="OpenItemBalance" source="BETRH"/></input>
  </calculationView>
