    <calculatedAttribute id="CC_CURRENT_ACTION" order="53" semanticType="empty" displayAttribute="false" attributeHierarchyActive="false">
      <descriptions defaultDescription="Current Action"/>
      <keyCalculation datatype="VARCHAR" expressionLanguage="SQL" length="30">
        <formula>CASE
    WHEN &quot;ZZCURRENT_ACTION&quot; LIKE '%ASSESSRAIS%' THEN 'Assessment Raised'
    WHEN &quot;ZZCURRENT_ACTION&quot; LIKE '%AUDITREQUI%' THEN 'Audit Required'
    WHEN &quot;ZZCURRENT_ACTION&quot; LIKE '%INFORECEIV%' THEN 'Information Received'
    WHEN &quot;ZZCURRENT_ACTION&quot; LIKE '%INFOREQUIR%' THEN 'Information Required'
    WHEN &quot;ZZCURRENT_ACTION&quot; LIKE '%NOACTIONRE%' THEN 'No Action Required'
    ELSE 'Not Applicable'
END</formula>
      </keyCalculation>
    </calculatedAttribute>
  </calculatedAttributes>
